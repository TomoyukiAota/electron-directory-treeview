    "watch": "chokidar . --ignore \"/^.git/\" --command \"npm test\""
  },
  "repository": "https://github.com/TomoyukiAota/photo-location-map",
  "author": "TomoyukiAota",
  "license": "MIT",
  "devDependencies": {
    "chokidar-cli": "1.2.0",
    "electron": "1.7.8",
    "electron-mocha": "5.0.0",
    "eslint": "4.13.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "intelli-espower-loader": "1.0.1",
    "npm-run-all": "4.1.2",
    "power-assert": "1.4.4"
  },
  "dependencies": {
    "directory-tree": "2.0.0",
    "electron-window-state": "4.1.1",
    "exif-parser": "0.1.12",
    "jquery": "3.2.1",
    "jstree": "3.3.4",
    "lodash": "4.17.4",
    "moment-timezone": "0.5.14"
  }
