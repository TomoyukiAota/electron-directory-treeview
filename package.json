{
  "name": "photo-location-map",
  "version": "0.1.0",
  "description": "Display the locations where your photos are taken in Google Maps.",
  "main": "src/main/main.js",
  "scripts": {
    "e2e-test": "mocha --require intelli-espower-loader ./test/e2e-test",
    "lint": "eslint .",
    "start": "electron .",
    "test": "npm-run-all lint unit-test",
    "unit-test": "electron-mocha --require intelli-espower-loader --recursive --renderer ./test/unit-test",
    "watch": "chokidar . --ignore \"/^.git/\" --command \"npm test\""
  },
  "repository": "https://github.com/TomoyukiAota/photo-location-map",
  "author": "TomoyukiAota",
  "license": "MIT",
  "devDependencies": {
    "chokidar-cli": "1.2.0",
    "electron": "1.8.8",
    "electron-mocha": "6.0.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.9.0",
    "intelli-espower-loader": "1.0.1",
    "mocha": "5.0.5",
    "npm-run-all": "4.1.2",
    "power-assert": "1.4.4",
    "spectron": "3.8.0"
  },
  "dependencies": {
    "directory-tree": "2.0.0",
    "electron-window-state": "4.1.1",
    "exif-parser": "0.1.12",
    "jquery": "3.2.1",
    "jstree": "3.3.4",
    "lodash": "^4.17.10",
    "moment-timezone": "0.5.14"
  }
}
